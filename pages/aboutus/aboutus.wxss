/* pages/aboutus/aboutus.wxss */

.contact-list {
  padding: 20rpx;
  text-align: left;
}

.t1 {
  margin: 20rpx 0;
  font-size: 17px;
  font-weight: 700;
  color: #000;
}

.t2 {
  margin: 10rpx 0;
  font-size: 14px;
  letter-spacing: 0.1em;
  color: #000;
}

.contact-img {
  height: 200px;
  overflow: hidden;
}

.contact-img image {
  display: block;
}

.footer {
  display: flex;
  justify-content: space-around;
  margin-top: 20px;
  font-weight: 700;
  text-align: center;
  color: tomato;
}
.desc {
  padding: 20rpx;
}
.desc-title {
  margin-bottom: 20rpx;
  color: tomato;
  font-size: 14px;
}
.desc-content {
  font-size: 14px;
  color: #666;
  text-indent: 2em;
}